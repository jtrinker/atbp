<div class="main-section">
		<div class="row container" style="padding-top: 40px;">
				<div class="large-12 columns text-center">
						<%= image_tag("atbp-main-logo.png", :alt => "are-the-barons-playing-main-logo") %>
				</div>
		</div>

				<div class="maintextwrap">
						<%= render "countdown_or_live" %>
				</div>
</div>

<div data-equalizer>

	<% if @todays_game && @todays_game.event.nil? %>
		<%= render "social_event_split" %>
	<% else %>
		<%= render "social_whole" %>
	<% end %>

	<div class="large-6 columns largeright" data-equalizer-watch>
		<%= render "schedule" %>
	</div>
</div>
