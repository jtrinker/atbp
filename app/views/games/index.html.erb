<div class="main-section">
		<div class="row container" style="padding-top: 40px;">
				<div class="large-12 columns text-center">
						<%= image_tag("atbp-main-logo.png", :alt => "are-the-barons-playing-main-logo") %>
				</div>
		</div>

		<div class="answer-wrap">

				<div class="maintextwrap">

						<h1 class="main-text answer" id="responsive_headline">YES!</h1>

						<h2 class="main-text2 gamewrap" id="responsive_headline2">
								<span class="next-venue">vs.</span>
								<span class="next-opponent">chattanooga</span> |
								<span class="next-time">12pm</span>
						</h2>
				</div>
		</div>
</div>

